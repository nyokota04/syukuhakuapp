<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<div class ="edit_view">
<%= form_for @user do |f| %>

  <%= f.label :ユーザー名 %>
  <%= f.text_field :user_name %>

  <%= f.label :メールアドレス %>
  <%= f.text_field :email %>

  <%= f.label :アイコン画像 %>
  <%= f.file_field :user_img %><br>

  <%= f.label :自己紹介 %>
  <%= f.text_field :profile %>

  <%= f.submit %>

  </div>

<% end %>
