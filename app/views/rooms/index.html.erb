<h1>Rooms#index</h1>
<p>Find me in app/views/rooms/index.html.erb</p>

<h1>ルーム一覧</h1>


<% @rooms.each do |room| %>
  <%= link_to room_path(room) do %>
    <p>ルーム画像</>
    <%= image_tag room.room_img %>
    <%= room.room_name %>
    <%= room.room_detail %>
    <%= room.price %>
    <%= room.address %>
  <% end %>
  <% end %>

  
